@model List<FichaOnline.Models.Peculiaridade>
@{
    ViewBag.Background = "transparent";
}
@if (Model.Count > 0)
{
    <div class="col-md-10 no-padding">
        <label class="text-form">Nome</label>
    </div>
    <div class="col-md-2 no-padding">
        <label class="text-form">Teste</label>
    </div>
    foreach (var p in Model)
    {
        <div class="col-md-10 no-padding">
            <span class="text-high-high" title="@p.Descricao">@p.Nome</span>
        </div>
        <div class="col-md-2 no-padding">
            <span class="text-high-high">@p.Teste.ToString("00")</span>
        </div>
    }
}
else
{
    <h1>Não há!</h1>
}